{
  "name": "template-serverless-localstack",
  "version": "1.0.0",
  "description": "Template Serverless + Localstack",
  "main": "serverless.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0mn1-Tech/template-serverless-localstack.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/0mn1-Tech/template-serverless-localstack/issues"
  },
  "homepage": "https://github.com/0mn1-Tech/template-serverless-localstack#readme",
  "scripts": {
    "pkg": "npx sls package",
    "debug": "SLS_DEBUG=* npx sls package",
    "remove": "npx sls remove",
    "deploy": "npx sls deploy --force",
    "eslint": "eslint . --ext js,ts --cache --fix",
    "prettier": "prettier --write './**/*.{js,ts}'",
    "invoke:hello": "npx sls invoke local -f hello --stage local -p fixtures/hello.json",
    "invoke:world": "npx sls invoke local -f world --stage local -p fixtures/world.json",
    "test:quick": "jest --maxWorkers=4 -c jest.config.ts --no-watchman -u",
    "test:html": "jest --collectCoverage --maxWorkers=4 -c jest.config.ts --colors --no-watchman --verbose --coverage --coverageDirectory './coverage'",
    "test-prepush": "jest --maxWorkers=50% -c jest.config.ts --colors --no-watchman",
    "test-changed": "jest --maxWorkers=50% -o -c jest.config.ts --colors --no-watchman",
    "localstack:start": "docker-compose up -d",
    "localstack:stop": "docker-compose down",
    "localstack:deploy": "sls deploy --aws-profile localstack",
    "locaLstack:logs": "docker-compose logs -f",
    "aws-sso-login": "aws sso login --profile taxis-dev",
    "sso-login": "NODE_PATH=./ ts-node -r tsconfig-paths/register ./scripts/sso-access.ts profile=taxis-dev",
    "deploy:dev": "npx sls deploy --aws-profile taxis-dev --stage dev"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "ajv": "^8.4.0",
    "aws-lambda": "^1.0.6",
    "class-transformer": "^0.4.0",
    "http-status-codes": "^2.1.4",
    "i18n": "^0.13.2",
    "mongoose": "^5.12.9",
    "pino": "^6.11.3",
    "serialize-error": "^8.1.0",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@schedulino/aws-lambda-test-utils": "^1.2.0",
    "@serverless/event-mocks": "^1.1.1",
    "@serverless/typescript": "^2.23.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/copy-webpack-plugin": "^8.0.0",
    "@types/i18n": "^0.13.0",
    "@types/ini": "^1.3.30",
    "@types/jest": "^26.0.23",
    "@types/mongoose": "^5.10.5",
    "@types/node": "^14.14.25",
    "@types/pino": "^6.3.8",
    "@types/webpack": "^5.1.0",
    "@types/webpack-node-externals": "^2.5.1",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "aws-sdk": "^2.908.0",
    "copy-webpack-plugin": "^8.1.1",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-module-resolver": "^1.2.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-sort-class-members": "^1.11.0",
    "husky": "^6.0.0",
    "ini": "^2.0.0",
    "jest": "^26.6.3",
    "json-schema-to-ts": "^1.5.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "serverless": "^2.23.0",
    "serverless-deployment-bucket": "^1.5.1",
    "serverless-localstack": "^0.4.30",
    "serverless-prune-plugin": "^1.5.0",
    "serverless-webpack": "^5.3.5",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.0.15",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "typescript-eslint": "^0.0.1-alpha.0",
    "webpack": "^5.1.0",
    "webpack-node-externals": "^2.5.2"
  }
}
